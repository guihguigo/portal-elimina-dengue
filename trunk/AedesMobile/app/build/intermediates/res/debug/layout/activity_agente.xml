<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:fab="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ListView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/lstAgente" />

    <com.github.clans.fab.FloatingActionButton
        android:id="@+id/fab_atualiza"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom|center"
        android:layout_marginBottom="30dp"
        android:layout_marginRight="65dp"
        android:src="@drawable/ic_action_navigation_refresh"
        fab:fab_colorNormal="@color/atualiza_normal"
        fab:fab_colorPressed="@color/atualiza_pressed"
        fab:fab_colorRipple="@color/atualiza_ripple"/>

    <com.github.clans.fab.FloatingActionButton
        android:id="@+id/fab_add"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom|center"
        android:layout_marginBottom="30dp"
        android:layout_marginLeft="65dp"
        android:src="@drawable/fab_add"
        android:onClick="adicionarPrevencao"
        fab:fab_colorNormal="@color/add_normal"
        fab:fab_colorPressed="@color/add_pressed"
        fab:fab_colorRipple="@color/add_ripple"/>
</FrameLayout><!-- From: file:/home/alexandre/Projeto/AedesMobile/app/src/main/res/layout/activity_agente.xml -->